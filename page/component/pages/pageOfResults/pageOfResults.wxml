<import src="../../../common/head.wxml" />
<import src="../../../common/foot.wxml" />

<view class="container">
  <template is="head" data="{{title: 'Results'}}"/>

  <view class="page-body">

    <view class="page-section" style='text-align:center;'>
      <text style='font-size:3ch;'>Your score: {{score}}</text>
    </view>
    
    
    <view class="btn-area">
      <button class="btn-center" type="primary" bindtap="onShareAppMessage" open-type="share" >Share </button>
    </view>
    
    <view class="page-body">
     <view class='mytext'>
      <text>About XiaoJoy</text>
      <text class='text-box'>
        Half of the music is generated by the AI trained on more than 20,000 midi music. \nAnd we select most of the human pieces from famous classic music composed by Mozart, Bach etc...
      </text>
     </view>
    </view>
   

  </view>
  <!-- <template is="foot" /> -->
</view>